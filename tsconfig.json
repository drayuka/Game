{
    "compilerOptions": {
      "allowJs": true,
      "module": "commonjs",
      "target": "es6",
      "lib": ["es2015"],
      "noImplicitAny": false,
      "sourceMap": false,
      "outDir": "./dist",
      "rootDir": "./new",
      "allowUnreachableCode": false,
      "strictFunctionTypes": false,
      "strictPropertyInitialization": false,
      "esModuleInterop": true
    },
    "include": [
      "./new/*",
      "./node_modules/@types/**/*.d.ts"
    ],
    "exclude": [
      "./new/test.ts"
    ]
}
